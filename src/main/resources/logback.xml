<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{36} - %msg%n</pattern>
		</encoder>
	</appender>

	<logger name="com.springframework" level="DEBUG" />
	<logger name="org.springframework.jdbc.datasource" level="DEBUG" />
	<logger name="org.springframework.transaction" level="DEBUG" />
	<!--<logger name="org.springframework.web.bind.annotation.support.HandlerMethodInvoker" level="INFO" />-->
	<!--<logger name="org.springframework.web.servlet.DispatcherServlet" leve="INFO" />-->
	
	<root level="DEBUG">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>